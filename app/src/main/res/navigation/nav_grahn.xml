<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_grahn"
    app:startDestination="@id/menuFragment">

    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.menuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menuFragment_to_comidaFragment"
            app:destination="@id/comidaFragment" />
        <action
            android:id="@+id/action_menuFragment_to_comprasFragment"
            app:destination="@id/comprasFragment" />
        <action
            android:id="@+id/action_menuFragment_to_perfilFragment"
            app:destination="@id/perfilFragment" />
        <action
            android:id="@+id/action_menuFragment_to_favoritosFragment"
            app:destination="@id/favoritosFragment" />
        <action
            android:id="@+id/action_menuFragment_to_rutaFragment"
            app:destination="@id/rutaFragment" />
        <action
            android:id="@+id/action_menuFragment_to_ayudaFragment"
            app:destination="@id/ayudaFragment" />
    </fragment>
    <fragment
        android:id="@+id/comidaFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.comidaFragment"
        android:label="fragment_comida"
        tools:layout="@layout/fragment_comida" />
    <fragment
        android:id="@+id/comprasFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.comprasFragment"
        android:label="fragment_compras"
        tools:layout="@layout/fragment_compras" />
    <fragment
        android:id="@+id/perfilFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.perfilFragment"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/favoritosFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.favoritosFragment"
        android:label="fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" />
    <fragment
        android:id="@+id/rutaFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.rutaFragment"
        android:label="fragment_ruta"
        tools:layout="@layout/fragment_ruta" />
    <fragment
        android:id="@+id/ayudaFragment"
        android:name="com.example.eltincho.Views.Ui.Fragments.ayudaFragment"
        android:label="fragment_ayuda"
        tools:layout="@layout/fragment_ayuda" />
</navigation>